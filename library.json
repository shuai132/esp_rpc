{
  "name": "esp_rpc",
  "description": "Tiny RPC Library for ESP8266/ESP32",
  "keywords": "rpc,tcp",
  "authors": {
    "name": "shuai132",
    "maintainer": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shuai132/esp_rpc.git"
  },
  "version": "2.0.0",
  "frameworks": "arduino",
  "platforms": [
    "espressif8266",
    "espressif32"
  ],
  "build": {
    "srcDir": "_",
    "flags": [
      "-I .",
      "-I rpc_core"
    ]
  },
  "dependencies": [
    {
      "owner": "me-no-dev",
      "name": "ESPAsyncTCP",
      "version": "^1.2.2",
      "platforms": "espressif8266"
    },
    {
      "owner": "me-no-dev",
      "name": "AsyncTCP",
      "version": "^1.1.1",
      "platforms": "espressif32"
    }
  ]
}
